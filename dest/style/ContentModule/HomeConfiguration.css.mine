 .layout_box {
 	box-sizing: content-box;
 	position: relative;
 	border: 1px solid #ccc;
 	border-radius: 20px 20px 0 0;
 	width: 375px;
 	min-height: 667px;
 	margin: 20px;
 	background: #FFF;
 	padding-bottom: 60px;
 	background: #f5f5f5;
 }
 /*头部*/
 
 .layout_box .layout_head {
 	height: 60px;
 	border-bottom: 1px solid #ddd;
 	position: relative;
 	background: #f5f5f5;
 	border-radius: 20px 20px 0 0;
 }
 
 .layout_box .layout_head:before {
 	width: 14px;
 	left: 120px;
 	content: '';
 	display: block;
 	height: 14px;
 	border-radius: 7px;
 	background: #ddd;
 	position: absolute;
 	top: 25px;
 }
 
 .layout_box .layout_head:after {
 	width: 100px;
 	left: 140px;
 	content: '';
 	display: block;
 	height: 14px;
 	border-radius: 7px;
 	background: #ddd;
 	position: absolute;
 	top: 25px;
 }
 /*头部*/
 /*nav部分*/
 
 .NavList {
 	width: 100%;
 	height: 100px;
 	background: #f53828;
 	padding-top: 50px;
 	position: relative;
 }
 
 .top_line {
 	position: absolute;
 	z-index: 99;
 	left: 0;
 	top: 5px;
 }
 
 .top_line img {
 	height: 40px;
 }
 
 #wrapper {
 	width: 100%;
 	height: 50px;
 	overflow: hidden;
 	position: relative;
 }
 
 .scroll {
 	width: 850px;
 	height: 50px;
 	position: absolute;
 	left: 0;
 	top: 0;
 }
 
 .scroll li {
 	width: 80px;
 	height: 50px;
 	line-height: 50px;
 	margin: 0 12px;
 	font-size: 20px;
 	text-align: center;
 	float: left;
 	overflow: hidden;
 	color: #f8847a;
 	cursor: pointer;
 	position: relative;
 }
 
 .scroll li span {
 	width: 100%;
 	height: 3px;
 	display: none;
 	background: #FFFFFF;
 	position: absolute;
 	bottom: 3px;
 	left: 1px;
 }
 
 .active {
 	color: #FFFFFF !important;
 }
 
 .nav_img_list label {
 	width: 100%;
 	height: 100%;
 }
 
 .nav_img_list label>span {
 	width: 15%;
 	height: 100%;
 	line-height: 100%;
 	margin: 0 5%;
 	overflow: hidden;
 	text-align: center;
 	display: inline-block;
 }
 
 .nav_img_list label>input {
 	width: 60%;
 }
 /*nav部分*/
 /*内容部分*/
 
 .layout_cont {
 	position: relative;
 	margin: 5px;
 	background: #EEE;
 }
 
 .layout_cont .cont_box {
 	position: relative;
 	box-sizing: content-box;
 	margin-top: 8px;
 }
 
 .top_line img {
 	width: 100%;
 	height: 100%;
 }
 
